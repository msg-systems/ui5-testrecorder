{
    "manifest_version": 3,
    "name": "UI5 Testrecorder",
    "short_name": "UI5 Test Rec.",
    "description": "A point-and-click adventure for your UI5 tests",
    "version": "0.0.1",
    "background": {
        "service_worker": "/scripts/starter.js"
    },
    "optional_permissions": [
        "tabs"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/scripts/page_inject.js",
                "/scripts/communication_inject.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "use_dynamic_url": true
        }
    ],
    "host_permissions": [
        "https://*/",
        "http://*/"
    ],
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ],
    "action": {},
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "64": "/images/64.png",
        "80": "/images/80.png",
        "96": "/images/96.png",
        "128": "/images/128.png"
    },
    "options_page": "/ui/options.html",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            }
        }
    }
}