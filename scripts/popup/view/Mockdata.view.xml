<mvc:View xmlns="sap.m" xmlns:f="sap.f" xmlns:mvc="sap.ui.core.mvc" controllerName="com.ui5.testing.controller.Mockdata">
    <f:DynamicPage headerExpanded="{/headerExpanded}" toggleHeaderOnTitleClick="{/titleClickable}">
        <!-- <f:title>
            <f:DynamicPageTitle>
            </f:DynamicPageTitle>
        </f:title> -->
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <Button text="GetV2DataModels" press="retrieveODataV2"/>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <IconTabBar class="sapUiResponsiveContentPadding" showOverflowSelectList="true" expanded="true" items="{mockdataModel>/eSets}" expandable="false">
                <items>
                    <IconTabFilter text="{mockdataModel>name}">
                        <Table columns="{mockdataModel>lineType/properties}">
                            <headerToolbar>
                                <OverflowToolbar>
                                    <ToolbarSpacer/>
                                    <Button icon="sap-icon://add" press="addLine"/>
                                </OverflowToolbar>
                            </headerToolbar>
                            <columns>
                                <Column>
                                    <Label text="{mockdataModel>name}" design="{path: 'mockdataModel>isKey', formatter: '.checkForKey'}"/>
                                </Column>
                            </columns>
                        </Table>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </f:content>
        <!-- DynamicPage Footer -->
        <f:footer>
            <OverflowToolbar>
            </OverflowToolbar>
        </f:footer>
    </f:DynamicPage>
</mvc:View>
