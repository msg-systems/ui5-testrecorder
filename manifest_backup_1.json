{
    "manifest_version": 3,
    "name": "UI5 Testrecorder",
    "short_name": "UI5 Test Rec.",
    "description": "A point-and-click adventure for your UI5 tests",
    "version": "0.0.1",
    "background": {
        "service_worker": "/scripts/starter.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "/ui/popup.html",
        "default_icon": {
            "16": "/images/16.png",
            "32": "/images/16.png",
            "48": "/images/16.png",
            "64": "/images/16.png",
            "80": "/images/16.png",
            "96": "/images/16.png",
            "128": "/images/16.png"
        }
    },
    "icons": {
        "16": "/images/16.png",
        "32": "/images/16.png",
        "48": "/images/16.png",
        "64": "/images/16.png",
        "80": "/images/16.png",
        "96": "/images/16.png",
        "128": "/images/16.png"
    },
    "options_page": "/ui/options.html",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            }
        }
    }
}